{
  "name": "@expressive-react/babel-plugin-auto-extends",
  "version": "1.7.0",
  "description": "Automatically extends React.Component for classes that include an Expressive-recognized render method.",
  "main": "lib/index.js",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "/lib"
  ],
  "scripts": {
    "prebuild": "rm -rf ./lib",
    "build": "babel ./src --out-dir ./lib",
    "dev": "babel ./src --out-dir ./lib --source-maps --watch",
    "prepare": "npm run build"
  },
  "dependencies": {
    "babel-types": "^6.25.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0"
  }
}
