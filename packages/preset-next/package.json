{
  "name": "@expressive-react/babel-preset-next",
  "version": "1.5.3",
  "description": "Expressive React support for Next.js applications",
  "main": "lib/index.js",
  "scripts": {
    "prebuild": "rm -rf ./lib",
    "build": "babel ./src --out-dir ./lib",
    "watch": "babel ./src --out-dir ./lib --source-maps --watch",
    "prepare": "npm run build"
  },
  "keywords": [
    "react",
    "expressive",
    "syntax",
    "next"
  ],
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "/lib"
  ],
  "dependencies": {
    "@expressive-react/babel-plugin-auto-extends": "^1.7.2",
    "@expressive-react/babel-plugin-transform-xjs": "^0.18.3",
    "@expressive-react/modify-web": "^1.8.2",
    "babel-preset-expressive-enhancements": "^1.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0"
  },
  "peerDependencies": {
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  }
}
