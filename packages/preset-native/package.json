{
  "name": "@expressive-react/babel-preset-native",
  "version": "1.5.2",
  "description": "Expressive-React preset for Native React",
  "main": "lib/index.js",
  "scripts": {
    "prebuild": "rm -rf ./lib",
    "build": "babel ./src --out-dir ./lib",
    "dev": "babel ./src --out-dir ./lib --source-maps --watch",
    "prepare": "npm run build"
  },
  "keywords": [
    "react",
    "expressive",
    "syntax"
  ],
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "/lib"
  ],
  "dependencies": {
    "@expressive-react/babel-plugin-auto-extends": "^1.7.1",
    "@expressive-react/babel-plugin-transform-xjs": "^0.18.2",
    "@expressive-react/modify-native": "^1.4.2",
    "babel-preset-expressive-enhancements": "^1.0.1",
    "babel-preset-react-native": "^4.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0"
  }
}
