{
  "presets": [
    [ "@babel/env", {
        "targets": {
            "node": "current"
        },
        "modules": "commonjs"
    }]
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-proposal-object-rest-spread"
  ],
  "sourceMaps": "inline",
  "compact": false
}